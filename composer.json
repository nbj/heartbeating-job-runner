{
    "name": "nbj/heartbeating-job-runner",
    "description": "Laravel Library for running commands with ZMQ heartbeats ",
    "type": "library",
    "license": "MIT",
    "require": {
        "nesbot/carbon": "*",
        "ext-zmq": "*",
        "nbj/stopwatch": "^0.0.4"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "friendsofphp/php-cs-fixer": "*",
        "orchestra/testbench": "^3.6"
    },
    "autoload": {
        "files": [
            "bootstrap/helpers.php"
        ],
        "psr-4": {
            "Nbj\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
